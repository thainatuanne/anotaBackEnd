{
  "version": 2,
  "builds": [
    {
      "src": "src/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/index.js"
    }
  ],
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "https://anota-back-end.vercel.app/$1"
    }
  ]
}